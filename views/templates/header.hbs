<nav>
  <h3>To make this website ,I have used axios module for fetching the amazon
    html page and cheerio module for scrapping the data</h3>
</nav>

<div class="outside">
  <div class="inner">
    <h3>Title</h3>
  </div>
  <div class="inner">
    <h3>Image</h3>
  </div>
  <div class="inner">
    <h3>Link</h3>
  </div>
  <div class="inner">
    <h3>Price</h3>
  </div>
  {{#each arr}}
    <div class="inner">
      <p>{{this.title}}</p>
    </div>
    <div class="inner">
      <p><a href="{{this.image}}">{{this.image}}</a></p>
    </div>
    <div class="inner">
      <p><a href="{{this.link}}">{{this.link}}</a></p>
    </div>
    <div class="inner">
      <p>{{this.price}}</p>
    </div>
  {{/each}}
</div>